        <member name="M:XIVCalc.Calculations.StatEquations.AutoAttackModifier(System.Int32,System.Int32,System.Int32,XIVCalc.Interfaces.IJobModifiers)">
            <summary>
            Like <see cref="M:XIVCalc.Calculations.StatEquations.WeaponDamageMultiplier(System.Int32,System.Int32,XIVCalc.Interfaces.IJobModifiers)"/>, but for auto-attacks.
            AkhMorning equivalent: f(AUTO)
            </summary>
            <param name="weaponDamage">Value of weapon damage</param>
            <param name="weaponDelay">Value of weapon delay</param>
            <param name="level">Current level</param>
            <param name="job"></param>
            <returns>Auto-attack damage multiplier f(AUTO)</returns>
        </member>
        <member name="M:XIVCalc.Calculations.StatEquations.BaseDamage(System.Int32,XIVCalc.Interfaces.IJobStatBlock,XIVCalc.AttackType,System.Boolean,System.Boolean,System.Boolean)">
